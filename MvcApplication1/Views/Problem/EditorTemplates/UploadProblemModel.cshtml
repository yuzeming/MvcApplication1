@model MvcApplication1.FormModels.UploadProblemModel
<div class="form-group">
    @Html.LabelFor( m => m.File)
    <input type="file" id="exampleInputFile" name="File" accept="application/zip">
    <p class="help-block">@ViewBag.Tip</p>
</div>

<div class="form-group">
    @Html.LabelFor( m => m.Title)
    @Html.TextBoxFor(m => m.Title, new { placeholder = "从压缩包中读取", @class = "form-control" })
</div>
<div class="form-group">
    <div class="checkbox">
        <label>
            @Html.CheckBoxFor( m => m.Public)
            @Html.LabelFor( m => m.Public)
        </label>
    </div>
</div>
<div class="form-group">
    <div class="checkbox">
        <label>
            @Html.CheckBoxFor( m => m.PublicData)
            @Html.LabelFor( m => m.PublicData)
        </label>
    </div>
</div>
