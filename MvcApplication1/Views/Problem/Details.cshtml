@model MvcApplication1.Models.Problem

@{
    ViewBag.Title = "Details";
}

<h1>@Model.Title</h1>
<div class="col-md-10">
@Html.Raw(@Model.Description)
</div>
<div class="col-md-2">
    <pre>CheckSum:@Model.CheckSum</pre>
    @Html.ActionLink("Edit", "Edit", new { id = Model.ID },new { @class = "btn btn-default" })
    @Html.ActionLink("Submit", "Create","Submit", new { id=Model.ID ,c = ViewBag.c},new { @class = "btn btn-default" })
</div>