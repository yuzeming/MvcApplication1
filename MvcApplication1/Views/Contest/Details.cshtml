@model MvcApplication1.Models.Contest

@{
    ViewBag.Title = "Details";
}

<h2>@Html.DisplayFor(model => model.Title)</h2>
        
<span>比赛时间 @Html.DisplayFor(model => model.Start) ~ @Html.DisplayFor(model => model.End)</span> 

<table class="table">
    <tr>
        <th>题号</th>
        <th>题目名</th>
        <th>操作</th>
    </tr>
    @foreach(var i in Model.ProbList)
    {
        <tr>
            <td>@i.ID</td>
            <td>@i.Title</td>
            <td>@Html.ActionLink("进入", "Details", "Problem", new { id = @i.ID , c = Model.ID }, new { })</td>
        </tr>
    }
</table>  

